{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "__DEV__": true,
    "toObjectId": true,
    "app": true,
    "grpc": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "no-restricted-syntax": "off",
    "comma-dangle": 0,
    "eqeqeq": [2, "allow-null"],
    "class-methods-use-this": 0,
    "id-length": 0,
    "strict": ["error", "global"],
    "no-unused-expressions": [2, { "allowShortCircuit": true, "allowTernary": true }],
    "prefer-template": 0,
    "radix": 0,
    "no-nested-ternary": 0,
    "no-debugger": 1,
    "max-len": ["error", 200, 2, {"ignoreUrls": true}],
    "no-param-reassign": [2, {"props": false}],
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "no-underscore-dangle": 0,
    "no-await-in-loop": 0,
    "no-plusplus": 0,
    "camelcase": 0,
    "no-console": 0,
    "valid-typeof": 0
  }
}